<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ObjectDetectionMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ObjectDetectionMaui"
    xmlns:views="clr-namespace:ObjectDetectionMaui.Views"
    Title="ObjectDetectionMaui">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Object Detection"
        ContentTemplate="{DataTemplate views:ObjectDetectionPage}"
        Route="ObjectDetection" />

</Shell>
